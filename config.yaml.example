# Aliyun Qwen-TTS-Realtime API Configuration
# Using Qwen-TTS-Realtime for larger free tier quota
aliyun:
  api_key: "your_api_key_here"

# TTS Settings
tts:
  model: "qwen3-tts-flash-realtime"  # Qwen TTS Realtime model
  voice: "Cherry"  # Popular voice options: Cherry, Serena, Ethan, Chelsie, etc.

# Audio Output Settings
audio:
  format: "mp3"
  bitrate: "64k"
  sample_rate: 24000

# Processing Settings
processing:
  max_concurrent: 1  # WebSocket is single connection per session
  chunk_size: 2000   # Characters per API request (max 2000)
  retry_attempts: 3
  retry_delay: 2     # Seconds
